{
  "name": "with-firebase-authentication",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev \"src/app\"",
    "preserve": "npm run preserve-app && npm run preserve-functions",
    "preserve-app": "npm run clean && npm run build-app",
    "preserve-functions": "npm run build-functions && npm run copy-functions-deps && npm run install-deps",
    "export": "next export \"src/app\" -o \"dist/public\"",
    "serve-app": "cross-env NODE_ENV=production firebase serve --only hosting",
    "serve-functions": "cross-env NODE_ENV=production firebase serve --only functions",
    "serve": "cross-env NODE_ENV=production firebase serve",
    "deploy": "npm run clean && firebase deploy --only functions,hosting",
    "clean": "rimraf \"dist\"",
    "build-app": "next build \"src/app\"",
    "build-app-permission": "chmod 755 dist/functions/next/static/media/*.svg",
    "build-public": "cpx \"src/public/**/*.*\" \"dist/public\" -C",
    "build-functions": "tsc --project src/functions",
    "lint-app": "tslint --project src/app",
    "typecheck-app": "tsc --project src/app",
    "lint-functions": "tsc --project src/functions",
    "copy-app-deps": "cpx \"*{package.json,package-lock.json,yarn.lock}\" \"dist/public\" -C",
    "copy-functions-deps": "cpx \"*{package.json,package-lock.json,yarn.lock}\" \"dist/functions\" -C",
    "install-deps": "cd \"dist/functions\" && npm i",
    "start": "next start"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.10.0",
    "@types/cors": "^2.8.8",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase": "^7.15.5",
    "firebase-admin": "^8.12.1",
    "firebase-functions": "^3.11.0",
    "js-cookie": "2.2.1",
    "next": "^9.5.4",
    "nookies": "^2.4.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-jss": "^10.4.0",
    "swr": "0.2.3"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/firebase": "^3.2.1",
    "@types/node": "^14.11.8",
    "@types/react": "^16.9.50",
    "@types/react-dom": "^16.9.8",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.2",
    "ts-loader": "^8.0.4",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  }
}
